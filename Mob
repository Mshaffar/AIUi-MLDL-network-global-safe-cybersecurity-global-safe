    <script type="text/javascript">
       
       // Add polyfill for IE
       (function () {
            if (typeof window.CustomEvent === 'function') return false;

            function CustomEvent(event, params) {
                params = params || { bubbles: false, cancelable: false, detail: null };
                var evt = document.createEvent('CustomEvent');
                evt.initCustomEvent(event, params.bubbles, params.cancelable, params.detail);
                return evt;
            }

            CustomEvent.prototype = window.Event.prototype;

            window.CustomEvent = CustomEvent;
            window.Event = CustomEvent;
        })();

        document.addEventListener("DOMContentLoaded", function(){
            AppleID.auth.init({
                clientId: 'com.mobisystems.officesuite.signin',
                scope: 'name email',
                redirectURI: 'https://accounts.mobisystems.com/',
                state: '5e52253c9d',
                usePopup: true
            });

            // Remove Apple Icon from button text
            var appleBtnContainter = $('.btn-apple').find('svg text');
            var appleBtnText = appleBtnContainter.text();
            appleBtnContainter.text(appleBtnText.substring(2, appleBtnText.length));
        });
    </script>

